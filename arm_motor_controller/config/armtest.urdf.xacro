<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="armtest">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import arm_three urdf file -->
    <xacro:include filename="$(find sw_arm_desc)/urdf/arm_three.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="armtest.ros2_control.xacro" />

    <xacro:arm_ros2_control name="arm" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
